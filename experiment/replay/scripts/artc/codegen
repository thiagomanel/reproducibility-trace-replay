#!/bin/bash

artc="compiler/artc"

traced_file=$1
init_file=$2

# when traced_file is /tmp/6.7200780.rr.4.10000.1000.4096.6.1.same-disk.load.out.artc.workload
# we have /tmp/6.7200780.rr.4.10000.1000.4096.6.1.same-disk.load.out.artc.workload-target outdir
outdir=$traced_file-target
mkdir $outdir

$artc --strace $traced_file $init_file $outdir
